# FA01BZ AI Agent Self-hosted Starter Kit
# 환경 변수 설정 예시
# 이 파일을 .env로 복사하고 필요에 따라 값을 수정하세요

# =================================
# 데이터베이스 설정 (PostgreSQL)
# =================================
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=n8n_password

# 추가 데이터베이스 사용자
AGENT_USER=agent_user
AGENT_PASSWORD=agent_pass
READONLY_USER=readonly_user
READONLY_PASSWORD=readonly_pass

# 추가 데이터베이스 이름
AGENTS_DB=fa01bz_agents
VECTOR_DB=vector_store
ANALYTICS_DB=analytics

# 환경 타입 (development/production)
ENVIRONMENT_TYPE=development

# =================================
# n8n 설정
# =================================
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678/
N8N_BASIC_AUTH_ACTIVE=false

# 시간대 설정 (한국 시간)
GENERIC_TIMEZONE=Asia/Seoul

# n8n 보안 설정 (선택사항)
# N8N_ENCRYPTION_KEY=your-encryption-key-here
# N8N_USER_MANAGEMENT_JWT_SECRET=your-jwt-secret-here

# =================================
# OpenWebUI 설정
# =================================
OLLAMA_BASE_URL=http://ollama:11434
WEBUI_AUTH=false
DEFAULT_MODELS=qwen2.5:3b,llama3.2:3b

# OpenWebUI 보안 키 (선택사항)
# WEBUI_SECRET_KEY=your-secret-key-here

# =================================
# Ollama 설정
# =================================
OLLAMA_ORIGINS=*
OLLAMA_HOST=0.0.0.0

# =================================
# 포트 설정
# =================================
N8N_PORT_HOST=5678
POSTGRES_PORT_HOST=5432
OLLAMA_PORT_HOST=11434
QDRANT_PORT_HOST=6333
OPENWEBUI_PORT_HOST=3000

# =================================
# 개발/디버깅 설정 (선택사항)
# =================================
# NODE_ENV=development
# N8N_LOG_LEVEL=debug

# =================================
# API 키 설정 (실습용)
# =================================
# OPENAI_API_KEY=your-openai-api-key
# GMAIL_CLIENT_ID=your-gmail-client-id
# GMAIL_CLIENT_SECRET=your-gmail-client-secret
# TELEGRAM_BOT_TOKEN=your-telegram-bot-token
# GOOGLE_CALENDAR_CLIENT_ID=your-calendar-client-id
# GOOGLE_CALENDAR_CLIENT_SECRET=your-calendar-client-secret
